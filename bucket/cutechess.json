{
    "version": "1.3.1",
    "description": "A graphical user interface, command-line interface and a library for playing chess.",
    "homepage": "https://cutechess.com/",
    "license": "GPL-3.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://github.com/cutechess/cutechess/releases/download/v1.3.1/cutechess-1.3.1-win64.zip",
            "extract_dir": "cutechess-1.3.1-win64"
        },
        "32bit": {
            "url": "https://github.com/cutechess/cutechess/releases/download/v1.3.1/cutechess-1.3.1-win32.zip",
            "extract_dir": "cutechess-1.3.1-win32"
        }
    },
    "shortcuts": [
        [
            "cutechess.exe",
            "CuteChess"
        ]
    ],
    "bin": [
        "cutechess-cli.exe"
    ],
    "checkver": {
        "github": "https://github.com/cutechess/cutechess",
        "regex": "v([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/cutechess/cutechess/releases/download/v$version/cutechess-$version-win64.zip",
                "extract_dir": "cutechess-$version-win64"
            },
            "32bit": {
                "url": "https://github.com/cutechess/cutechess/releases/download/v$version/cutechess-$version-win32.zip",
                "extract_dir": "cutechess-$version-win32"
            }
        }
    }
}
